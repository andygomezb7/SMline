{if $image.i_status == 0 && $user->permits['vfb']}<div class="item-info error" style="margin-bottom:5px">Esta imagen se encuentra actualmente <b>eliminada</b>. Tus privilegios de rango te permiten visualizar imágenes eliminadas.</div>{/if}<div class="post_title">{$image.i_title}</div>	<div class="post-head-info clearfix">		<span class="post_date"><a class="stip" title="{$image.i_date|date_format:"%d/%m/%Y %I:%M %p"}">{$image.i_date|hace}</a></span>		<div class="actions_follow_and_fav">			<div id="follows-buttons" data-type="images" type-id="{$image.i_id}" style="display:inline-block;">				<a class="button_1 to_follow require-login" style="{if $user->is_following('image', $image.i_id)}display:none;{/if}"><img src="{$web.icons}/follow.png"/>Seguir imagen</a>				<a class="button_3 ok_follow" style="{if !$user->is_following('image', $image.i_id)}display:none;{/if}">Siguiendo</a>				<a class="b_cancel un_follow" style="display:none;">Dejar de seguir</a>			</div>						{if $image.i_user != $user->uid && !$user->permits['goadmin'] && !$user->permits['gomod']}			<a class="button_1 type_5 stip require-login" onclick="user.new_report('imagen', '{$image.i_title}', '{$image.u_nick}', {$image.i_id});" title="Reportar imagen"><img src="{$web.icons}/flag.png" /></a>			{/if}						{if $image.i_user != $user->uid}			<a onclick="{if $user->uid}img.fav(0);return false;{/if}" class="button_1 post_add_fav require-login" style="{if $image.i_is_fav}display:none;{/if}"><img src="{$web.icons}/heart_10.png"/>Agregar a favoritos</a>			<a onclick="img.fav(1);" class="button_1 post_remove_fav" style="{if !$image.i_is_fav}display:none;{/if}"><img src="{$web.icons}/heart_10.png"/>Borrar de favoritos</a>			{/if}						{if ($image.u_id == $user->uid && ($user->permits['bfp'] || $user->permits['efp'])) || $user->permits['bf'] || $user->permits['ef']}			<a class="button_1 post_options" onclick="$('.list_options').toggle(400);">Opciones</a>			<ul class="list_options">				{if $image.i_status == 0 && $user->permits['vfb']}					<li><a onclick="mod.reac('images', {$image.i_id});"><img src="{$web.icons}/sok.png"/> Reactivar imagen</a></li>				{elseif $image.u_id == $user->uid && $user->permits['bfp']}					<li><a onclick="img.del({$image.i_id});"><img src="{$web.icons}/delete.png"/> Borrar imagen</a></li>				{elseif $user->permits['bf']}					<li><a onclick="mod.del('images', {$image.i_id});"><img src="{$web.icons}/delete.png"/> Borrar imagen</a></li>				{/if}				{if ($image.u_id == $user->uid && $user->permits['efp']) || $user->permits['ef']}					<li><a href="{$web.url}/imagenes/agregar?imgid={$image.i_id}"><img src="{$web.icons}/edit.png"/> Editar imagen</a></li>				{/if}			</ul>			{/if}					</div>	</div>	{if $image.history}	<div class="post-mod-history">		<center><h3>Historial de moderación de la imagen			<a onclick="$('.post-mod-history table').toggle();" class="floatR" title="Ocultar/Mostrar"><i class="icon expand"></i></a>		</h3></center>		<table cellpadding="0" cellspacing="0" border="0" class="admin_table" width="718" align="center">            <thead>				<tr>					<th>Moderador</th>					<th>Acción</th>					<th>Razón</th>					<th>Fecha</th>				</tr>			</thead>			<tbody>				{foreach from=$image.history item=h}				<tr>					<td><a href="{$web.url}/{$h.u_nick}"><b>{$h.u_nick}</b></a></td>					<td>						{if $h.h_action == 1}<span class="color-red">borró la imagen</span> 						{elseif $h.h_action == 2}<span class="color-green">reactivó la imagen</span>						{elseif $h.h_action == 3}<span class="color-gray">editó la imagen</span>						{/if}					</td>					<td>{$h.h_reason}</td>					<td>{$h.h_date|hace}</td>				</tr>				{/foreach}			</tbody>		</table>	</div>	{/if}		<div class="box_body post_content margin-top-5 clearfix">		<center>			<img src="{$image.i_url}" class="img-full" alt="{$image.i_title}" title="{$image.i_title}"/>			<span class="img_desc">{$image.i_description}</span>		</center>		<hr class="hr-dashed"/>								<span class="share-title">Recomienda esta imagen a tus amigos.</span>		<script type="text/javascript">var switchTo5x=true;</script>		<script type="text/javascript" src="http://w.sharethis.com/button/buttons.js"></script>		<script type="text/javascript">{literal}stLight.options({publisher: "f9eaeb68-1fbe-46ad-9970-5ad61d3aa753", doNotHash: false, doNotCopy: false, hashAddressBar: false});{/literal}</script>		<div class="post-share-bottom">			<span class="st_email_hcount" displayText="Email"></span>			<span class="st_linkedin_hcount" displayText="LinkedIn"></span>			<span class="st_googleplus_hcount" displayText="Google +"></span>			<span class="st_twitter_hcount" displayText="Tweet"></span>			<span class="st_facebook_hcount" displayText="Facebook"></span>			<span class="number-share-t unrated">				<a onclick="{if $user->uid}img.share();return false;{/if}" href="#" class="share-t require-login"></a>				<span class="count-shareds">					<span class="shar-pico"></span>					<span class="total-share">{$image.i_shared}</span>				</span>			</span>		</div>		{*if $user->permits['vpf'] || $user->permits['vnf'] || !$user->uid*}		<div class="dar-voto floatR">			<span class="item-info ok margin-top-5" style="display:none"><img src="{$web.icons}/ok.png"> Voto agregado!</span>			<center>				<span class="share-title center">Califica esta imagen.</span>			<a class="likes_buttons ilike stip require-login vote-type-1" onclick="{if $user->uid}img.vote(1, $(this));return false;{/if}" href="#" title="Me gusta">				<span class="ilike_back"></span>				<span class="ilike_text">{$image.i_positives}</span>			</a>			<a class="likes_buttons idislike stip require-login vote-type-0" onclick="{if $user->uid}img.vote(0, $(this));return false;{/if}" href="#" title="No me gusta">				<span class="ilike_back"></span>				<span class="ilike_text">{$image.i_negatives}</span>			</a>			</center>		</div>			</div>	<div class="post-head-info pbottom margin-top-10 clearfix">		<ul class="post-metadata">			<li class="floatL">				<i class="icon heart"></i><b id="post-total-favs">{$image.i_favs}</b> Favoritos			</li>			<li class="floatL">				<i class="icon comments"></i><b id="post-total-comment">{$image.i_comments}</b> Comentarios			</li>			<li class="floatL">				<i class="icon eye"></i><b>{$image.i_follows}</b> Seguidores			</li>			<li class="floatL">				<i class="icon hits"></i><b>{$image.i_hits}</b> Visitas			</li>		</ul>	</div>