<div class="box margin-top-5">	<div class="box_title">Comentarios recientes</div>	<div class="box_body list_element">		{if $last_comments}		{foreach from=$last_comments item=i}		<div class="list-element">			<a href="{$web.url}/{$i.u_nick}" class="subinfo">{$i.u_nick}</a>			<a href="{$web.url}/imagenes/{$i.i_id}/{$i.i_title|seo}.html">{$i.i_title}</a>		</div>		{/foreach}		{else}		<div class="emptyData">No hay comentarios nuevos en im&aacute;genes</div>		{/if}	</div></div>