<div class="box margin-top-5">	<div class="box_title">Top comunidades</div>		<div class="box_filter filter_comus clearfix">		<a onclick="tops_filter('comus', 1, 'ho')" class="f_p_ho">Hoy</a>		<a onclick="tops_filter('comus', 2, 'ay')" class="f_p_ay">Ayer</a>		<a onclick="tops_filter('comus', 3, 'se')" class="f_p_se">Semana</a>		<a onclick="tops_filter('comus', 4, 'me')" class="f_p_me">Mes</a>		<a onclick="tops_filter('comus', 5, 'hi')" class="f_p_hi active">Histórico</a>	</div>		<div class="box_body list_element top_comus">		{if !$top_comus}<div class="emptyData">No hay comunidades en este rango de tiempo</div>{/if}		{foreach from=$top_comus item=c key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<a href="{$web.url}/comunidades/{$c.comu_seo}/" title="{$c.comu_name}">{$c.comu_name|substr:0:28}</a>			<span class="value">{$c.comu_members}</span>		</div>		{/foreach}		<div class="box_more">			<a href="{$web.url}/tops/comunidades">Ver más</a>		</div>	</div></div>