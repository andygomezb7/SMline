<div class="box margin-top-5">	<div class="box_title">Últimos miembros</div>	<div class="box_body">		{foreach from=$comu_members item=u}		<li class="list_users">			<a href="{$web.url}/{$u.u_nick}" class="hovercard" uid="{$u.u_id}" title="{$u.u_nick}"><img src="{$web.avatar}/{$u.u_id}_32.jpg?{$u.u_last_avatar}" width="31px" /></a>		</li>		{/foreach}		{if $me.m_rank == 1 || $me.m_rank == 2 || $me.m_user == $comu.comu_admin || $user->permits.ecs}		<div class="box_more">			<a href="{$web.url}/comunidades/{$comu.comu_seo}/miembros/">Ver más</a>		</div>		{/if}	</div></div>