<div class="box">	<div class="box_title">Staff</div>	<div class="box_body">		{foreach from=$comu_staff item=u}		<li class="list_users {if $u.m_rank == 1}admin{else}mod{/if}">			<a href="{$web.url}/{$u.u_nick}" class="hovercard" uid="{$u.u_id}" title="{$u.u_nick}"><img src="{$web.avatar}/{$u.u_id}_50.jpg?{$u.u_last_avatar}" width="43px" /></a>			{if !$user->is_online($u.u_id)}<span title="Offline" class="user-status offline stip"></span>			{else}<span title="Online" class="user-status online stip"></span>			{/if}		</li>		{/foreach}	</div></div>