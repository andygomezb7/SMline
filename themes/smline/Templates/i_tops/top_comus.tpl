<div class="box box-top">	<div class="box_title">Comunidades con más miembros</div>		<div class="box_body list_element">		{if !$comus_populares}<div class="emptyData">Nada por aqu&iacute;</div>{/if}		{foreach from=$comus_populares item=co key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<i class="icon" title="{$co.c_name}" style="background: url({$web.icons}/comus-cats/{$co.c_img}) no-repeat;"></i>			<a href="{$web.url}/comunidades/{$co.comu_seo}/" title="{$co.comu_title}">{$co.comu_name}</a>			<span class="value">{$co.total}</span>		</div>		{/foreach}	</div></div><div class="box box-top">	<div class="box_title">Comunidades con más temas</div>		<div class="box_body list_element">		{if !$comus_temas}<div class="emptyData">Nada por aqu&iacute;</div>{/if}		{foreach from=$comus_temas item=co key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<i class="icon" title="{$co.c_name}" style="background: url({$web.icons}/comus-cats/{$co.c_img}) no-repeat;"></i>			<a href="{$web.url}/comunidades/{$co.comu_seo}/" title="{$co.comu_title}">{$co.comu_name}</a>			<span class="value">{$co.total}</span>		</div>		{/foreach}	</div></div><div class="box margin-top-5 box-top">	<div class="box_title">Comunidades con más seguidores</div>		<div class="box_body list_element">		{if !$comus_seguidores}<div class="emptyData">Nada por aqu&iacute;</div>{/if}		{foreach from=$comus_seguidores item=co key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<i class="icon" title="{$co.c_name}" style="background: url({$web.icons}/comus-cats/{$co.c_img}) no-repeat;"></i>			<a href="{$web.url}/comunidades/{$co.comu_seo}/" title="{$co.comu_title}">{$co.comu_name}</a>			<span class="value">{$co.total}</span>		</div>		{/foreach}	</div></div>