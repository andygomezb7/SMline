<div class="box box-top">	<div class="box_title">Usuarios con m치s puntos</div>		<div class="box_body list_element">		{if !$users_puntos}<div class="emptyData">Nada por aqu&iacute;</div>{/if}		{foreach from=$users_puntos item=u key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<img src="{$web.url}/avatar/{$u.u_id}_16.jpg?{$u.u_last_avatar}" />			<a href="{$web.url}/{$u.u_nick}">{$u.u_nick}</a>			<span class="value">{$u.total}</span>		</div>		{/foreach}	</div></div><div class="box box-top">	<div class="box_title">Usuarios con m치s seguidores</div>		<div class="box_body list_element">		{if !$users_seguidores}<div class="emptyData">Nada por aqu&iacute;</div>{/if}		{foreach from=$users_seguidores item=u key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<img src="{$web.url}/avatar/{$u.u_id}_16.jpg?{$u.u_last_avatar}" />			<a href="{$web.url}/{$u.u_nick}">{$u.u_nick}</a>			<span class="value">{$u.total}</span>		</div>		{/foreach}	</div></div><div class="box margin-top-5 box-top">	<div class="box_title">Usuarios con m치s comentarios</div>		<div class="box_body list_element">		{if !$users_comentarios}<div class="emptyData">Nada por aqu&iacute;</div>{/if}		{foreach from=$users_comentarios item=u key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<img src="{$web.url}/avatar/{$u.u_id}_16.jpg?{$u.u_last_avatar}" />			<a href="{$web.url}/{$u.u_nick}">{$u.u_nick}</a>			<span class="value">{$u.total}</span>		</div>		{/foreach}	</div></div><div class="box margin-top-5 box-top">	<div class="box_title">Usuarios con m치s posts</div>		<div class="box_body list_element">		{if !$users_posts}<div class="emptyData">Nada por aqu&iacute;</div>{/if}		{foreach from=$users_posts item=u key=i}		<div class="list-element">			<span class="number-list">{$i+1}</span>			<img src="{$web.url}/avatar/{$u.u_id}_16.jpg?{$u.u_last_avatar}" />			<a href="{$web.url}/{$u.u_